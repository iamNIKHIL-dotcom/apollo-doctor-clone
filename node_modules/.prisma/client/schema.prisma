generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Doctor {
  id              Int      @id @default(autoincrement())
  name            String
  specialization  String
  location        String
  experience      Int
  availability    String
  consultationFee Float
  createdAt       DateTime @default(now())
}
